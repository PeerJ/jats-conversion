  <!-- ********* Grant / award identifiers *********-->

  <xsd:element name="award-number" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation>Funder-assigned award or grant number</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="award-start-date" type="xsd:date">
    <xsd:annotation>
      <xsd:documentation>Dates award is created</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="doi_data">
    <xsd:annotation>
      <xsd:documentation>Wrapper for DOI and URL(s)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="doi"/>
        <xsd:element ref="resource"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="doi">
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
        <xsd:maxLength value="2048"/>
        <xsd:minLength value="6"/>
        <xsd:pattern value="10\.[0-9]{4,9}/.{1,200}"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="resource" type="xsd:anyURI"/>
</xsd:schema>
